{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "prefer-arrow-callback": 0,
        "func-names": 0,
        "quotes": 0,
        "max-len": 0,
        "no-console": 0,
        "prefer-const": 0,
        "prefer-template": 0,
        "object-shorthand": 0,
        "comma-dangle": 0,
        "no-restricted-syntax": 0,
        "no-param-reassign": 0,
        "strict": 0,
        "no-shadow": 0,
        "global-require": 0,
        "arrow-body-style": 0,
        "no-else-return": 0,
        "consistent-return": 0
    }
}
